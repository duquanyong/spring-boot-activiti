      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="theTask" id="BPMNShape_theTask">
        <omgdc:Bounds height="60.0" width="100.0" x="280.0" y="101.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="theEnd" id="BPMNShape_theEnd">
        <omgdc:Bounds height="35.0" width="35.0" x="460.0" y="112.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge bpmnElement="flow1" id="BPMNEdge_flow1">
        <omgdi:waypoint x="197.0" y="130.0"></omgdi:waypoint>
        <omgdi:waypoint x="212.0" y="130.0"></omgdi:waypoint>
        <omgdi:waypoint x="212.0" y="130.0"></omgdi:waypoint>
        <omgdi:waypoint x="280.0" y="131.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow2" id="BPMNEdge_flow2">
        <omgdi:waypoint x="380.0" y="131.0"></omgdi:waypoint>
        <omgdi:waypoint x="383.0" y="129.0"></omgdi:waypoint>
        <omgdi:waypoint x="460.0" y="129.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
